                    android:layout_height="match_parent"
                    android:layout_weight="105"
                    android:background="@color/white"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/tv_system_voltage_set_title"
                        style="@style/tv_parameter_set_param_name_title_style"
                        android:text="@string/system_voltage_set" />

                    <EditText
