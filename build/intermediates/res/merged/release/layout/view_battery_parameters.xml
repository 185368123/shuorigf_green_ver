                    android:layout_height="match_parent"
                    android:layout_weight="145"
                     android:visibility="gone"
                    android:background="@color/white"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/tv_nominal_capacity_of_battery_set_title"
                        style="@style/tv_parameter_set_param_name_title_style"
                        android:text="@string/nominal_capacity_of_battery_set" />

                    <EditText
                        android:id="@+id/tv_parameter_set_nominal_capacity_of_battery_set_value"
                        style="@style/tv_parameter_set_param_value_title_style"
                        android:text="200" />
                </LinearLayout>

                <View
