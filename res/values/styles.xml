<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Theme" parent="android:Theme"></style>
    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="PageTitle">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textColor">#ff363636</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="light_tab">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">12sp</item>
        <item name="android:paddingTop">1dip</item>
        <item name="android:paddingBottom">3dip</item>
        <item name="android:background">@drawable/bottombar_itembg_selector</item>
        <item name="android:textColor">@color/bottombar_textcolor_selector</item>
        <item name="android:button">@null</item>
    </style>

    <style name="Theme.Transparent">

        <!-- 设置允许窗口半透明 -->

        <item name="android:windowIsTranslucent">true</item>

        <!-- 引用Android系统的动画半透明样式 -->

        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>

        <!-- 设定窗口背景颜色 -->

        <item name="android:windowBackground">@color/transparent_background</item>

        <!-- 隐藏窗口标题栏 -->

        <item name="android:windowNoTitle">true</item>

        <!-- 设定文字前景色 -->

        <item name="android:colorForeground">#fff</item>
    </style>

